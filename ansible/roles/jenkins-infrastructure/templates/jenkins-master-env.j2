# Environment file for Jenkins Master {{ item }}
# Generated by Ansible - do not edit manually

# Java Options
JAVA_OPTS="{{ jenkins_master_java_opts }}"

# Jenkins Options  
JENKINS_OPTS="--httpPort={{ jenkins_port }}"

# JCasC Configuration
CASC_JENKINS_CONFIG="{{ jenkins_home_dir }}/master-{{ item }}/casc"

# Harbor Registry Configuration
HARBOR_REGISTRY_URL="{{ harbor_registry_url | default('docker.io') }}"
HARBOR_PROJECT_NAME="{{ harbor_project | default('jenkins') }}"

# Version Information
JENKINS_VERSION="{{ jenkins_master_image_tag | default('lts') }}"
JENKINS_AGENT_VERSION="{{ jenkins_agent_version | default('latest') }}"

# System Configuration
TZ="{{ common_timezone | default('UTC') }}"

# Container Runtime
CONTAINER_RUNTIME="{{ jenkins_container_runtime }}"
JENKINS_INSTANCE="{{ item }}"