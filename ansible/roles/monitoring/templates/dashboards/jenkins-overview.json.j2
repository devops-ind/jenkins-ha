{
  "dashboard": {
    "id": null,
    "title": "Jenkins Overview - {{ deployment_environment | default('local') | title }}",
    "tags": ["jenkins", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Jenkins Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"jenkins\"}",
            "legendFormat": "Jenkins Up"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Build Queue Length",
        "type": "stat",
        "targets": [
          {
            "expr": "jenkins_queue_size_value",
            "legendFormat": "Queue Size"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      }
    ],
    "version": 1
  },
  "overwrite": true
}