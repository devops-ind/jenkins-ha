{
  "dashboard": {
    "id": null,
{% if dashboard_team is defined %}
    "title": "{% if dashboard_title_override is defined and dashboard_title_override %}{{ dashboard_title_override }}{% else %}Jenkins Overview - {{ dashboard_team | title }} Team{% endif %}",
    "tags": ["jenkins", "overview", "team-specific", "{{ dashboard_team }}", "{{ team_environment | default('blue') }}", "{{ deployment_mode | default('local') }}"],
{% else %}
    "title": "{% if dashboard_title_override is defined and dashboard_title_override %}{{ dashboard_title_override }}{% else %}Jenkins Overview - {{ deployment_mode | default('local') | title }}{% endif %}",
    "tags": ["jenkins", "overview", "multi-team", "{{ deployment_mode | default('local') }}"],
{% endif %}
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
{% if dashboard_team is defined %}
    "templating": {
      "list": []
    },
{% else %}
    "templating": {
      "list": [
        {
          "name": "jenkins_team",
          "type": "query",
          "query": "label_values(up{job=~\"jenkins.*\"}, jenkins_team)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Team"
        },
        {
          "name": "jenkins_environment",
          "type": "query",
          "query": "label_values(up{job=~\"jenkins.*\"}, jenkins_environment)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Environment"
        }
      ]
    },
{% endif %}
    "panels": [
      {
        "id": 1,
        "title": "Jenkins Status",
        "type": "stat",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "up{job=~\"jenkins.*\", jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}",
            "legendFormat": "{{ dashboard_team | title }} Team ({{ team_environment | default('blue') }})"
{% else %}
            "expr": "up{job=~\"jenkins.*\", jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} ({{ '{{' }}jenkins_environment{{ '}}' }})"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Build Queue Length",
        "type": "stat",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "jenkins_queue_size_value{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}",
            "legendFormat": "{{ dashboard_team | title }} Team Queue"
{% else %}
            "expr": "jenkins_queue_size_value{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Queue"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Active Builds",
        "type": "stat",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "jenkins_builds_running_builds{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}",
            "legendFormat": "{{ dashboard_team | title }} Team Active"
{% else %}
            "expr": "jenkins_builds_running_builds{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Active"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Error Rate (Last 5min)",
        "type": "stat",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "rate(jenkins_builds_failure_build_count{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}[5m])",
            "legendFormat": "{{ dashboard_team | title }} Team Errors"
{% else %}
            "expr": "rate(jenkins_builds_failure_build_count{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}[5m])",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Errors"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Jenkins Build Logs - Recent Activity",
        "type": "logs",
        "datasource": "Loki",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "{job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |= \"BUILD\"",
            "refId": "A",
            "legendFormat": "{{ dashboard_team | title }} Build Logs"
{% else %}
            "expr": "{job=\"jenkins\", team=~\"$jenkins_team\"} |= \"BUILD\"",
            "refId": "A",
            "legendFormat": "Build Logs"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      },
      {
        "id": 6,
        "title": "Error Logs - Critical Issues",
        "type": "logs",
        "datasource": "Loki",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "{job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |~ \"(?i:error|exception|failed|failure)\"",
            "refId": "A",
            "legendFormat": "{{ dashboard_team | title }} Errors"
{% else %}
            "expr": "{job=\"jenkins\", team=~\"$jenkins_team\"} |~ \"(?i:error|exception|failed|failure)\"",
            "refId": "A",
            "legendFormat": "Error Logs"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "exact",
          "sortOrder": "Descending"
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "list"
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Build Success vs Failure Rate (5min)",
        "type": "timeseries",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "rate(jenkins_builds_success_build_count{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}[5m])",
            "legendFormat": "{{ dashboard_team | title }} Success Rate",
            "refId": "A"
{% else %}
            "expr": "rate(jenkins_builds_success_build_count{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}[5m])",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Success Rate",
            "refId": "A"
{% endif %}
          },
          {
{% if dashboard_team is defined %}
            "expr": "rate(jenkins_builds_failure_build_count{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}[5m])",
            "legendFormat": "{{ dashboard_team | title }} Failure Rate",
            "refId": "B"
{% else %}
            "expr": "rate(jenkins_builds_failure_build_count{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}[5m])",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Failure Rate",
            "refId": "B"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "color": {
              "mode": "palette-classic"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "ops"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Failure.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Success.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "green"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 8,
        "title": "Log Volume by Level",
        "type": "stat",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "count_over_time({job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |~ \"(?i)error\" [5m])",
            "legendFormat": "{{ dashboard_team | title }} Errors",
            "refId": "A"
{% else %}
            "expr": "count_over_time({job=\"jenkins\", team=~\"$jenkins_team\"} |~ \"(?i)error\" [5m])",
            "legendFormat": "Errors",
            "refId": "A"
{% endif %}
          },
          {
{% if dashboard_team is defined %}
            "expr": "count_over_time({job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |~ \"(?i)warn\" [5m])",
            "legendFormat": "{{ dashboard_team | title }} Warnings",
            "refId": "B"
{% else %}
            "expr": "count_over_time({job=\"jenkins\", team=~\"$jenkins_team\"} |~ \"(?i)warn\" [5m])",
            "legendFormat": "Warnings",
            "refId": "B"
{% endif %}
          },
          {
{% if dashboard_team is defined %}
            "expr": "count_over_time({job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |~ \"(?i)info\" [5m])",
            "legendFormat": "{{ dashboard_team | title }} Info",
            "refId": "C"
{% else %}
            "expr": "count_over_time({job=\"jenkins\", team=~\"$jenkins_team\"} |~ \"(?i)info\" [5m])",
            "legendFormat": "Info",
            "refId": "C"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "color": {
              "mode": "thresholds"
            },
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Error.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Warning.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "yellow"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Info.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "blue"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 9,
        "title": "Security Events and Authentication Logs",
        "type": "logs",
        "datasource": "Loki",
        "targets": [
          {
            "expr": "{job=\"auth\"} |~ \"(?i:failed|successful|login|logout|authentication)\"",
            "refId": "A",
            "legendFormat": "Security Events"
          },
          {
{% if dashboard_team is defined %}
            "expr": "{job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |~ \"(?i:login|logout|authentication|security|unauthorized)\"",
            "refId": "B",
            "legendFormat": "{{ dashboard_team | title }} Security"
{% else %}
            "expr": "{job=\"jenkins\", team=~\"$jenkins_team\"} |~ \"(?i:login|logout|authentication|security|unauthorized)\"",
            "refId": "B",
            "legendFormat": "Jenkins Security"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 20
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "exact",
          "sortOrder": "Descending"
        }
      },
      {
        "id": 10,
        "title": "HAProxy Load Balancer Logs",
        "type": "logs",
        "datasource": "Loki",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "{job=\"haproxy\", team=\"{{ dashboard_team }}\"}",
            "refId": "A",
            "legendFormat": "{{ dashboard_team | title }} HAProxy"
{% else %}
            "expr": "{job=\"haproxy\"}",
            "refId": "A",
            "legendFormat": "HAProxy Logs"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 28
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      }
    ],
    "version": 1
  },
  "overwrite": true
}