{
  "dashboard": {
    "id": null,
    "title": "Jenkins Overview - {{ deployment_environment | default('local') | title }}",
    "tags": ["jenkins", "overview", "multi-team"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "jenkins_team",
          "type": "query",
          "query": "label_values(up{job=~\"jenkins.*\"}, jenkins_team)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Team"
        },
        {
          "name": "jenkins_environment",
          "type": "query",
          "query": "label_values(up{job=~\"jenkins.*\"}, jenkins_environment)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Environment"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Jenkins Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"jenkins.*\", jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} ({{ '{{' }}jenkins_environment{{ '}}' }})"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Build Queue Length",
        "type": "stat",
        "targets": [
          {
            "expr": "jenkins_queue_size_value{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Queue"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      }
    ],
    "version": 1
  },
  "overwrite": true
}