{
  "dashboard": {
    "id": null,
    "title": "Jenkins Dynamic Agents - {{ deployment_mode | default('local') | title }}",
    "tags": ["jenkins", "agents", "dynamic", "containers", "{{ deployment_mode | default('local') }}"],
    "style": "dark",
    "timezone": "browser", 
    "refresh": "15s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "jenkins_team",
          "type": "query",
          "query": "label_values(container_cpu_usage_seconds_total{container_label_com_jenkins_agent=\"true\"}, container_label_team)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Jenkins Team",
          "sort": 1
        },
        {
          "name": "agent_type",
          "type": "query", 
          "query": "label_values(container_cpu_usage_seconds_total{container_label_com_jenkins_agent=\"true\", container_label_team=~\"$jenkins_team\"}, container_label_agent_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Agent Type",
          "sort": 1
        },
        {
          "name": "deployment_mode",
          "type": "constant",
          "query": "{{ deployment_mode | default('local') }}",
          "current": {
            "text": "{{ deployment_mode | default('local') }}",
            "value": "{{ deployment_mode | default('local') }}"
          },
          "label": "Deployment Mode",
          "hide": 0
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Agent Status Overview",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "count(container_last_seen{container_label_com_jenkins_agent=\"true\", container_label_team=~\"$jenkins_team\", container_label_agent_type=~\"$agent_type\"} > (time() - 300))",
            "refId": "A",
            "legendFormat": "Active Agents"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow", 
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 5
                }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "horizontal"
        }
      },
      {
        "id": 2,
        "title": "Agents by Team",
        "type": "piechart",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "count by (container_label_team) (container_last_seen{container_label_com_jenkins_agent=\"true\", jenkins_team=~\"$jenkins_team\"} > (time() - 300))",
            "refId": "A",
            "legendFormat": "{{ '{{' }}container_label_team{{ '}}' }}"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "list",
            "placement": "right"
          }
        }
      },
      {
        "id": 3,
        "title": "Agents by Type",
        "type": "piechart",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "count by (container_label_agent_type) (container_last_seen{container_label_com_jenkins_agent=\"true\", jenkins_team=~\"$jenkins_team\", agent_type=~\"$agent_type\"} > (time() - 300))",
            "refId": "A",
            "legendFormat": "{{ '{{' }}container_label_agent_type{{ '}}' }}"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "pieType": "donut",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "list",
            "placement": "right"
          }
        }
      },
      {
        "id": 4,
        "title": "Agent Queue Depth",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(jenkins_queue_size_value{jenkins_team=~\"$jenkins_team\"})",
            "refId": "A",
            "legendFormat": "Queue Depth"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
