      },
      {
        "id": 5,
        "title": "Agent Provisioning Rate",
        "type": "timeseries",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "targets": [
          {
            "expr": "rate(jenkins_agent_launches_total{jenkins_team=~\"$jenkins_team\", agent_type=~\"$agent_type\"}[5m]) * 60",
            "refId": "A",
            "legendFormat": "{{ '{{' }}container_label_team{{ '}}' }}-{{ '{{' }}container_label_agent_type{{ '}}' }}"
          }
