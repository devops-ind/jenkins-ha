    "templating": {
      "list": [
        {
          "name": "jenkins_team",
          "type": "query",
          "query": "label_values(up{job=~\"jenkins.*\"}, jenkins_team)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Team"
        },
        {
          "name": "jenkins_environment",
          "type": "query",
          "query": "label_values(up{job=~\"jenkins.*\"}, jenkins_environment)",
