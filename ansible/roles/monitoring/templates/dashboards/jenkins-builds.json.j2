{
  "dashboard": {
    "id": null,
{% if dashboard_team is defined %}
    "title": "{{ dashboard_title_override | default('Jenkins Builds - ' + dashboard_team | title + ' Team') }}",
    "tags": ["jenkins", "builds", "team-specific", "{{ dashboard_team }}", "{{ team_environment | default('blue') }}", "{{ deployment_mode | default('local') }}"],
{% else %}
    "title": "Jenkins Builds - {{ deployment_mode | default('local') | title }}",
    "tags": ["jenkins", "builds", "multi-team", "{{ deployment_mode | default('local') }}"],
{% endif %}
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
{% if dashboard_team is defined %}
    "templating": {
      "list": []
    },
{% else %}
    "templating": {
      "list": [
        {
          "name": "jenkins_team",
          "type": "query",
          "query": "label_values(up{job=~\"jenkins.*\"}, jenkins_team)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Team"
        },
        {
          "name": "jenkins_environment",
          "type": "query",
          "query": "label_values(up{job=~\"jenkins.*\"}, jenkins_environment)",
          "refresh": 1,
          "includeAll": true,
          "multi": true,
          "allValue": ".*",
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "options": [],
          "label": "Environment"
        }
      ]
    },
{% endif %}
    "panels": [
      {
        "id": 1,
        "title": "Build Rate",
        "type": "timeseries",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "rate(jenkins_builds_total{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}[5m])",
            "legendFormat": "{{ dashboard_team | title }} Team Build Rate"
{% else %}
            "expr": "rate(jenkins_builds_total{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}[5m])",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Build Rate"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              }
            },
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Build Success vs Failure Rate",
        "type": "timeseries",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "rate(jenkins_builds_success_build_count{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}[5m])",
            "legendFormat": "{{ dashboard_team | title }} Success Rate",
            "refId": "A"
{% else %}
            "expr": "rate(jenkins_builds_success_build_count{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}[5m])",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Success Rate",
            "refId": "A"
{% endif %}
          },
          {
{% if dashboard_team is defined %}
            "expr": "rate(jenkins_builds_failure_build_count{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}[5m])",
            "legendFormat": "{{ dashboard_team | title }} Failure Rate",
            "refId": "B"
{% else %}
            "expr": "rate(jenkins_builds_failure_build_count{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}[5m])",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Failure Rate",
            "refId": "B"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "stacking": {
                "mode": "none",
                "group": "A"
              }
            },
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Failure.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Success.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "green"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 3,
        "title": "Build Duration Trends",
        "type": "timeseries",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "jenkins_builds_last_build_duration_milliseconds{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}/ 1000",
            "legendFormat": "{{ dashboard_team | title }} {{ '{{' }}job{{ '}}' }} Duration"
{% else %}
            "expr": "jenkins_builds_last_build_duration_milliseconds{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}/ 1000",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} {{ '{{' }}job{{ '}}' }} Duration"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0,
              "gradientMode": "none",
              "showPoints": "auto",
              "pointSize": 5
            },
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Build Logs - Success Events",
        "type": "logs",
        "datasource": "Loki",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "{job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |~ \"(?i)success|successful|completed|finished\" | line_format \"{{ '{{' }}.timestamp{{ '}}' }} [{{ '{{' }}.container_name{{ '}}' }}] {{ '{{' }}.output{{ '}}' }}\"",
            "refId": "A",
            "legendFormat": "{{ dashboard_team | title }} Success Logs"
{% else %}
            "expr": "{job=\"jenkins\", team=~\"$jenkins_team\"} |~ \"(?i)success|successful|completed|finished\" | line_format \"{{ '{{' }}.timestamp{{ '}}' }} [{{ '{{' }}.team{{ '}}' }}] {{ '{{' }}.output{{ '}}' }}\"",
            "refId": "A",
            "legendFormat": "Success Logs"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "exact",
          "sortOrder": "Descending"
        }
      },
      {
        "id": 5,
        "title": "Build Logs - Failure Events",
        "type": "logs",
        "datasource": "Loki",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "{job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |~ \"(?i)error|exception|failed|failure|abort\" | line_format \"{{ '{{' }}.timestamp{{ '}}' }} [{{ '{{' }}.container_name{{ '}}' }}] {{ '{{' }}.output{{ '}}' }}\"",
            "refId": "A",
            "legendFormat": "{{ dashboard_team | title }} Failure Logs"
{% else %}
            "expr": "{job=\"jenkins\", team=~\"$jenkins_team\"} |~ \"(?i)error|exception|failed|failure|abort\" | line_format \"{{ '{{' }}.timestamp{{ '}}' }} [{{ '{{' }}.team{{ '}}' }}] {{ '{{' }}.output{{ '}}' }}\"",
            "refId": "A",
            "legendFormat": "Failure Logs"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "exact",
          "sortOrder": "Descending"
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "list"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Build Log Volume vs Failure Rate Correlation",
        "type": "timeseries",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "rate(jenkins_builds_failure_build_count{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}[5m])",
            "legendFormat": "{{ dashboard_team | title }} Build Failure Rate",
            "refId": "A"
{% else %}
            "expr": "rate(jenkins_builds_failure_build_count{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}[5m])",
            "legendFormat": "{{ '{{' }}jenkins_team{{ '}}' }} Build Failure Rate",
            "refId": "A"
{% endif %}
          },
          {
{% if dashboard_team is defined %}
            "expr": "rate(count_over_time({job=\"jenkins\", team=\"{{ dashboard_team }}\", environment=\"{{ team_environment | default('blue') }}\"} |~ \"(?i)error|exception|failed\" [1m])[5m:])",
            "legendFormat": "{{ dashboard_team | title }} Error Log Rate",
            "refId": "B",
            "datasource": "Loki"
{% else %}
            "expr": "rate(count_over_time({job=\"jenkins\", team=~\"$jenkins_team\"} |~ \"(?i)error|exception|failed\" [1m])[5m:])",
            "legendFormat": "{{ '{{' }}team{{ '}}' }} Error Log Rate",
            "refId": "B",
            "datasource": "Loki"
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "stacking": {
                "mode": "none",
                "group": "A"
              }
            },
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Error.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Failure.*"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "orange"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 7,
        "title": "Job-Specific Build Analysis",
        "type": "table",
        "targets": [
          {
{% if dashboard_team is defined %}
            "expr": "jenkins_builds_last_build_result{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"}",
            "legendFormat": "{{ '{{' }}job{{ '}}' }}",
            "refId": "A",
            "format": "table",
            "instant": true
{% else %}
            "expr": "jenkins_builds_last_build_result{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"}",
            "legendFormat": "{{ '{{' }}job{{ '}}' }}",
            "refId": "A",
            "format": "table",
            "instant": true
{% endif %}
          },
          {
{% if dashboard_team is defined %}
            "expr": "jenkins_builds_last_build_duration_milliseconds{jenkins_team=\"{{ dashboard_team }}\", jenkins_environment=\"{{ team_environment | default('blue') }}\"} / 1000",
            "legendFormat": "{{ '{{' }}job{{ '}}' }}",
            "refId": "B",
            "format": "table",
            "instant": true
{% else %}
            "expr": "jenkins_builds_last_build_duration_milliseconds{jenkins_team=~\"$jenkins_team\", jenkins_environment=~\"$jenkins_environment\"} / 1000",
            "legendFormat": "{{ '{{' }}job{{ '}}' }}",
            "refId": "B",
            "format": "table",
            "instant": true
{% endif %}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "list",
              "filterable": true
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "SUCCESS",
                    "color": "green"
                  },
                  "1": {
                    "text": "UNSTABLE",
                    "color": "yellow"
                  },
                  "2": {
                    "text": "FAILURE",
                    "color": "red"
                  },
                  "3": {
                    "text": "NOT_BUILT",
                    "color": "blue"
                  },
                  "4": {
                    "text": "ABORTED",
                    "color": "purple"
                  }
                },
                "type": "value"
              }
            ]
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Value #A"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Last Build Result"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Value #B"
              },
              "properties": [
                {
                  "id": "displayName",
                  "value": "Last Build Duration (s)"
                },
                {
                  "id": "unit",
                  "value": "s"
                }
              ]
            }
          ]
        },
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "instance": true
              },
              "indexByName": {
                "job": 0,
                "jenkins_team": 1,
                "jenkins_environment": 2,
                "Value #A": 3,
                "Value #B": 4
              },
              "renameByName": {
                "job": "Job Name",
                "jenkins_team": "Team",
                "jenkins_environment": "Environment"
              }
            }
          }
        ]
      }
    ],
    "version": 1
  },
  "overwrite": true
}