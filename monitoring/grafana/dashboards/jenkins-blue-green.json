{
  "dashboard": {
    "id": null,
    "title": "Jenkins Blue-Green Deployment Dashboard",
    "tags": ["jenkins", "blue-green", "deployment"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "ðŸ”„ Blue-Green Environment Status",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Environment Status by Team",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background",
              "align": "center"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Active Environment"},
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {"options": {"blue": {"text": "BLUE", "color": "blue"}}, "type": "value"},
                    {"options": {"green": {"text": "GREEN", "color": "green"}}, "type": "value"}
                  ]
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Health Status"},
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {"options": {"1": {"text": "Healthy", "color": "green"}}, "type": "value"},
                    {"options": {"0": {"text": "Unhealthy", "color": "red"}}, "type": "value"}
                  ]
                }
              ]
            }
          ]
        },
        "targets": [
          {
            "expr": "jenkins_blue_green_active_environment",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "renameByName": {
                "team": "Team",
                "environment": "Active Environment",
                "instance": "Instance",
                "Value": "Health Status"
              }
            }
          }
        ]
      },
      {
        "id": 3,
        "title": "Environment Switch History",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 1},
        "targets": [
          {
            "expr": "increase(jenkins_blue_green_switch_total[1h])",
            "legendFormat": "Switches - {{team}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Switch Count", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 4,
        "title": "ðŸš€ Deployment Metrics",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9}
      },
      {
        "id": 5,
        "title": "Switch Success Rate",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 10},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "targets": [
          {
            "expr": "(jenkins_blue_green_switch_success_total / jenkins_blue_green_switch_total) * 100",
            "legendFormat": "Switch Success Rate",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Average Switch Duration",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 10},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 30},
                {"color": "red", "value": 60}
              ]
            },
            "unit": "s",
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "jenkins_blue_green_switch_duration_seconds",
            "legendFormat": "Switch Duration",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Rollback Rate",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 10},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 15}
              ]
            },
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "(jenkins_blue_green_rollback_total / jenkins_blue_green_switch_total) * 100",
            "legendFormat": "Rollback Rate",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Time Since Last Switch",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 10},
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "targets": [
          {
            "expr": "time() - jenkins_blue_green_last_switch_timestamp",
            "legendFormat": "Time Since Last Switch - {{team}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "ðŸ“Š Health Check Metrics",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 10,
        "title": "Health Check Response Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 17},
        "targets": [
          {
            "expr": "jenkins_blue_green_health_check_duration_seconds",
            "legendFormat": "Health Check Duration - {{environment}} ({{team}})",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Duration (s)", "min": 0},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "30s",
          "handler": 1,
          "name": "Slow Health Checks",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 11,
        "title": "Health Check Failure Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 17},
        "targets": [
          {
            "expr": "rate(jenkins_blue_green_health_check_failures_total[5m])",
            "legendFormat": "Failure Rate - {{environment}} ({{team}})",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Failures/sec", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 12,
        "title": "ðŸ”„ Traffic Management",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 25}
      },
      {
        "id": 13,
        "title": "Traffic Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 26},
        "targets": [
          {
            "expr": "jenkins_blue_green_traffic_percentage",
            "legendFormat": "{{environment}} - {{team}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "pie",
          "displayLabels": ["name", "value"],
          "unit": "percent"
        }
      },
      {
        "id": 14,
        "title": "Request Rate by Environment",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 26},
        "targets": [
          {
            "expr": "rate(jenkins_http_requests_total[5m])",
            "legendFormat": "{{environment}} - {{team}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 15,
        "title": "Connection Pool Status",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 26},
        "targets": [
          {
            "expr": "jenkins_blue_green_connection_pool_active",
            "legendFormat": "Active Connections - {{environment}} ({{team}})",
            "refId": "A"
          },
          {
            "expr": "jenkins_blue_green_connection_pool_idle",
            "legendFormat": "Idle Connections - {{environment}} ({{team}})",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Connections", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 16,
        "title": "ðŸš¨ Alerts and Issues",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 34}
      },
      {
        "id": 17,
        "title": "Switch Failures (Last 24h)",
        "type": "table",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 35},
        "targets": [
          {
            "expr": "increase(jenkins_blue_green_switch_failures_total[24h])",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "renameByName": {
                "team": "Team",
                "reason": "Failure Reason",
                "environment": "Target Environment",
                "Value": "Failure Count"
              }
            }
          }
        ]
      },
      {
        "id": 18,
        "title": "Environment Drift Detection",
        "type": "stat",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 35},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "jenkins_blue_green_configuration_drift_detected",
            "legendFormat": "Drift Count - {{team}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 19,
        "title": "ðŸ“ˆ Performance Comparison",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 41}
      },
      {
        "id": 20,
        "title": "Response Time Comparison",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 42},
        "targets": [
          {
            "expr": "jenkins_http_request_duration_seconds{environment=\"blue\"}",
            "legendFormat": "Blue Environment - P{{quantile}}",
            "refId": "A"
          },
          {
            "expr": "jenkins_http_request_duration_seconds{environment=\"green\"}",
            "legendFormat": "Green Environment - P{{quantile}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Response Time (s)", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 21,
        "title": "Error Rate Comparison",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 42},
        "targets": [
          {
            "expr": "rate(jenkins_http_requests_total{environment=\"blue\",status=~\"5..\"}[5m]) / rate(jenkins_http_requests_total{environment=\"blue\"}[5m]) * 100",
            "legendFormat": "Blue Environment Error Rate",
            "refId": "A"
          },
          {
            "expr": "rate(jenkins_http_requests_total{environment=\"green\",status=~\"5..\"}[5m]) / rate(jenkins_http_requests_total{environment=\"green\"}[5m]) * 100",
            "legendFormat": "Green Environment Error Rate",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Error Rate (%)", "min": 0},
          {"show": false}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1,
    "uid": "jenkins-blue-green-deployment"
  },
  "overwrite": true
}