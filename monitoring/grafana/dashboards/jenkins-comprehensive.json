{
  "dashboard": {
    "id": null,
    "title": "Jenkins HA Infrastructure - Comprehensive Dashboard",
    "tags": ["jenkins", "ha", "infrastructure", "sli"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "üéØ Service Level Indicators (SLIs)",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Deployment Success Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "targets": [
          {
            "expr": "jenkins:deployment_success_rate_5m",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate (5m)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "percent",
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "jenkins:error_rate_5m",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Response Time P95",
        "type": "stat", 
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 2000}
              ]
            },
            "unit": "ms",
            "min": 0
          }
        },
        "targets": [
          {
            "expr": "jenkins:response_time_p95",
            "legendFormat": "P95 Response Time",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Service Availability",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 99.5},
                {"color": "green", "value": 99.9}
              ]
            },
            "unit": "percent",
            "min": 90,
            "max": 100
          }
        },
        "targets": [
          {
            "expr": "jenkins:service_availability * 100",
            "legendFormat": "Availability",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "üöÄ Blue-Green Deployment Status",
        "type": "row", 
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9}
      },
      {
        "id": 7,
        "title": "Active Environment Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "color-background",
              "align": "center"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Status"},
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {"options": {"1": {"text": "Active", "color": "green"}}, "type": "value"},
                    {"options": {"0": {"text": "Inactive", "color": "red"}}, "type": "value"}
                  ]
                }
              ]
            }
          ]
        },
        "targets": [
          {
            "expr": "jenkins_blue_green_active_environment",
            "format": "table",
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "renameByName": {
                "instance": "Instance",
                "team": "Team", 
                "environment": "Environment",
                "Value": "Status"
              }
            }
          }
        ]
      },
      {
        "id": 8,
        "title": "Deployment Frequency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
        "targets": [
          {
            "expr": "jenkins:deployment_frequency_per_day",
            "legendFormat": "Deployments/Day - {{team}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Deployments", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 9,
        "title": "üìä Build and Queue Metrics",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18}
      },
      {
        "id": 10,
        "title": "Build Queue Length",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 19},
        "targets": [
          {
            "expr": "jenkins_queue_size_value",
            "legendFormat": "Queue Size - {{instance}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Queue Items", "min": 0},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Build Queue",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 11,
        "title": "Active Executors",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 19},
        "targets": [
          {
            "expr": "jenkins_executor_in_use_value",
            "legendFormat": "In Use - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "jenkins_executor_count_value", 
            "legendFormat": "Total - {{instance}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Executors", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 12,
        "title": "Build Success vs Failure Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 19},
        "targets": [
          {
            "expr": "rate(jenkins_builds_success_build_count[5m])",
            "legendFormat": "Success Rate - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "rate(jenkins_builds_failure_build_count[5m])",
            "legendFormat": "Failure Rate - {{instance}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Builds/sec", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 13,
        "title": "üê≥ Container and Agent Metrics",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 27}
      },
      {
        "id": 14,
        "title": "Dynamic Agent Utilization",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "jenkins_agent_launched_total",
            "legendFormat": "Agents Launched - {{type}}",
            "refId": "A"
          },
          {
            "expr": "jenkins_agent_terminated_total",
            "legendFormat": "Agents Terminated - {{type}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Agent Count", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 15,
        "title": "Container Resource Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\"jenkins.*\"}[5m]) * 100",
            "legendFormat": "CPU % - {{name}}",
            "refId": "A"
          },
          {
            "expr": "container_memory_usage_bytes{name=~\"jenkins.*\"} / 1024 / 1024 / 1024",
            "legendFormat": "Memory GB - {{name}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "CPU % / Memory GB"},
          {"show": false}
        ]
      },
      {
        "id": 16,
        "title": "üîí Security and Monitoring",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 36}
      },
      {
        "id": 17,
        "title": "Failed Login Attempts",
        "type": "stat",
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 37},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "rate(jenkins_login_failure_total[5m]) * 300",
            "legendFormat": "Failed Logins/5m",
            "refId": "A"
          }
        ]
      },
      {
        "id": 18,
        "title": "System Load Average",
        "type": "graph",
        "gridPos": {"h": 6, "w": 9, "x": 6, "y": 37},
        "targets": [
          {
            "expr": "node_load1",
            "legendFormat": "1m - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "node_load5",
            "legendFormat": "5m - {{instance}}", 
            "refId": "B"
          },
          {
            "expr": "node_load15",
            "legendFormat": "15m - {{instance}}",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Load", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 19,
        "title": "Disk Space Usage",
        "type": "graph",
        "gridPos": {"h": 6, "w": 9, "x": 15, "y": 37},
        "targets": [
          {
            "expr": "(1 - (node_filesystem_avail_bytes{fstype!=\"tmpfs\"} / node_filesystem_size_bytes{fstype!=\"tmpfs\"})) * 100",
            "legendFormat": "Disk Usage % - {{mountpoint}} ({{instance}})",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Usage %", "min": 0, "max": 100},
          {"show": false}
        ]
      },
      {
        "id": 20,
        "title": "üìà DORA Metrics",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 43}
      },
      {
        "id": 21,
        "title": "Mean Time to Recovery (MTTR)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 44},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 4}
              ]
            },
            "unit": "h"
          }
        },
        "targets": [
          {
            "expr": "jenkins:deployment_mttr_hours",
            "legendFormat": "MTTR",
            "refId": "A"
          }
        ]
      },
      {
        "id": 22,
        "title": "Change Failure Rate (7d)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 44},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 20}
              ]
            },
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "jenkins:change_failure_rate_7d",
            "legendFormat": "Change Failure Rate",
            "refId": "A"
          }
        ]
      },
      {
        "id": 23,
        "title": "Lead Time for Changes",
        "type": "graph",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 44},
        "targets": [
          {
            "expr": "jenkins_build_duration_milliseconds_summary{quantile=\"0.5\"} / 1000 / 60",
            "legendFormat": "P50 Build Time (min)",
            "refId": "A"
          },
          {
            "expr": "jenkins_build_duration_milliseconds_summary{quantile=\"0.95\"} / 1000 / 60",
            "legendFormat": "P95 Build Time (min)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Minutes", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 24,
        "title": "Deployment Frequency Trend",
        "type": "graph",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 44},
        "targets": [
          {
            "expr": "increase(jenkins_deployment_success_total[1d])",
            "legendFormat": "Daily Deployments - {{team}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Deployments", "min": 0},
          {"show": false}
        ]
      },
      {
        "id": 25,
        "title": "üö® Recent Alerts and Events",
        "type": "row",
        "collapsed": false,
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 52}
      },
      {
        "id": 26,
        "title": "Active Alerts",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 53},
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "format": "table",
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true, "alertstate": true},
              "renameByName": {
                "alertname": "Alert",
                "severity": "Severity",
                "instance": "Instance",
                "summary": "Summary"
              }
            }
          }
        ]
      }
    ],
    "time": {
      "from": "now-6h", 
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "uid": "jenkins-ha-comprehensive"
  },
  "overwrite": true
}