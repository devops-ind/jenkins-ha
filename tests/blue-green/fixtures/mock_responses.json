    "rollback_scenario": {
      "description": "Automatic rollback due to high error rate",
      "initial_state": {
        "devops_environment": "blue"
      },
      "switch_operation": {
        "team": "devops",
        "from_environment": "blue",
        "to_environment": "green"
      },
      "failure_condition": {
        "error_rate": 0.15,
        "response_time_ms": 8000
      },
      "expected_result": {
        "success": false,
        "rollback_executed": true,
        "final_environment": "blue",
        "rollback_reason": "High error rate detected"
      }
